					<Button x:Name="btnBlockMaxList" Content="最大数の変更" HorizontalAlignment="Center" Margin="0,365,0,0" VerticalAlignment="Top" Width="160" Height="22" Click="btnBlockMaxList_Click"/>
				</Grid>
			</TabItem>
			<TabItem Header="エネミー">
				<Grid Background="#FFE5E5E5"/>
			</TabItem>
			<TabItem Header="オブジェクト">
				<Grid Background="#FFE5E5E5">
					<Grid.ColumnDefinitions>
						<ColumnDefinition Width="89*"/>
						<ColumnDefinition Width="283*"/>
					</Grid.ColumnDefinitions>
					<ListBox x:Name="lsbObject" Height="337" Margin="6,10,10,0" VerticalAlignment="Top" HorizontalAlignment="Center" Width="162" SelectionChanged="lsbObject_SelectionChanged"/>
					<Button x:Name="btnObjectMaxList" Content="最大数の変更" HorizontalAlignment="Center" Margin="9,360,9,0" VerticalAlignment="Top" Width="160" Height="22" Click="btnObjectMaxList_Click"/>
					<TextBox x:Name="txbObjectName" Grid.Column="1" HorizontalAlignment="Left" Height="24" Margin="111,14,0,0" TextWrapping="Wrap" Text="なまえ" VerticalAlignment="Top" Width="109" VerticalContentAlignment="Center" KeyDown="txbObjectName_KeyDown"/>
					<Label x:Name="lblObjectName" Content="名前：" Grid.Column="1" HorizontalAlignment="Left" Margin="17,10,0,0" VerticalAlignment="Top" Background="White" Height="32" Width="539" Panel.ZIndex="-1" Padding="5,7,5,0"/>
