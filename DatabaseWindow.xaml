					<Label x:Name="lblPlayerSpriteName_Copy6" Content="死亡" HorizontalAlignment="Left" Margin="558,259,0,0" VerticalAlignment="Top" Background="White" Height="26" Width="133" HorizontalContentAlignment="Center" Panel.ZIndex="2"/>
