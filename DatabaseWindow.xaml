					<TextBox x:Name="txbObjDurability" Grid.Column="1" HorizontalAlignment="Left" Height="20" Margin="117,234,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="64" KeyDown="txbObjDurability_KeyDown"/>
					<TextBox x:Name="txbTime" Grid.Column="1" HorizontalAlignment="Left" Height="20" Margin="271,234,0,0" TextWrapping="Wrap" Text="1000" VerticalAlignment="Top" Width="64" KeyDown="txbTime_KeyDown"/>
					<Label x:Name="label_Copy" Content="Y" Grid.Column="1" HorizontalAlignment="Left" Margin="206,284,0,0" VerticalAlignment="Top" Background="#FFE0E0E0" Width="64"/>
					<Label x:Name="label_Copy1" Content="速度" Grid.Column="1" HorizontalAlignment="Left" Margin="280,284,0,0" VerticalAlignment="Top" Background="#FFE0E0E0" Width="78"/>
					<TextBox x:Name="txbObjAutoSpeed" Grid.Column="1" HorizontalAlignment="Left" Height="20" Margin="314,287,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="40" HorizontalContentAlignment="Right" Panel.ZIndex="2" KeyDown="txbObjAutoSpeed_KeyDown"/>
