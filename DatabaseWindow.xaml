					<TextBox x:Name="txbObjInfluenceJump" Grid.Column="1" HorizontalAlignment="Left" Height="20" Margin="210,396,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="40" HorizontalContentAlignment="Right" Panel.ZIndex="2" KeyDown="txbObjInfluenceJump_KeyDown"/>
					<Label x:Name="label_Copy3" Content="ジャンプ力" Grid.Column="1" HorizontalAlignment="Left" Margin="149,393,0,0" VerticalAlignment="Top" Background="#FFE0E0E0" Width="106"/>
					<CheckBox x:Name="ckbObjSlide" Content="すべり" Grid.Column="1" HorizontalAlignment="Left" Margin="200,263,0,0" VerticalAlignment="Top" Click="ckbObjSlide_Click"/>
					<CheckBox x:Name="ckbObjTriggerType" Content="OFF：接触、ON：入力）" Grid.Column="1" HorizontalAlignment="Left" Margin="155,322,0,0" VerticalAlignment="Top" Panel.ZIndex="1" Click="ckbObjTriggerType_Click"/>
