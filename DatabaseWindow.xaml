			<TabItem Header="アイテム">
				<Grid Background="#FFE5E5E5"/>
			</TabItem>
		</TabControl>
		<Button x:Name="btnUpdate" Content="適用" HorizontalAlignment="Left" Height="24" Margin="710,0,0,3" Grid.Row="1" VerticalAlignment="Center" Width="64" Click="btnUpdate_Click"/>

	</Grid>
</Window>
