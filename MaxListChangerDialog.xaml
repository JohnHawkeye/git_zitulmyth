<Window x:Class="Zitulmyth.MaxListChangerDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Zitulmyth"
        mc:Ignorable="d"
        Title="最大数変更" Height="124" Width="328" Background="Black" ResizeMode="NoResize" WindowStartupLocation="CenterScreen">
	<Grid>
		<TextBox x:Name="txbNumber" InputMethod.IsInputMethodSuspended="True" HorizontalAlignment="Left" Height="22" Margin="128,14,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="64" VerticalContentAlignment="Center" PreviewTextInput="txbNumber_PreviewTextInput" Panel.ZIndex="2"/>
		<Label x:Name="label" Content="最大数(1-255)：" HorizontalAlignment="Left" Margin="10,12,0,0" VerticalAlignment="Top" Background="#FFD6D6D6" Width="196" Height="26" Padding="5,4,5,0" Panel.ZIndex="1"/>
		<Button x:Name="btnOk" Content="OK" HorizontalAlignment="Left" Margin="228,12,0,0" VerticalAlignment="Top" Width="75" Height="22" Click="btnOk_Click"/>
		<Button x:Name="btnCancel" Content="キャンセル" HorizontalAlignment="Left" Margin="228,55,0,0" VerticalAlignment="Top" Width="75" Height="22" Click="btnCancel_Click"/>

	</Grid>
</Window>
