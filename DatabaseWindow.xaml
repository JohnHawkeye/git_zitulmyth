					<TextBox x:Name="txbBlockName" Grid.Column="1" HorizontalAlignment="Left" Height="24" Margin="104,14,0,0" TextWrapping="Wrap" Text="なまえ" VerticalAlignment="Top" Width="109" VerticalContentAlignment="Center" KeyDown="txbBlockName_KeyDown"/>
					<Label x:Name="lblBlockName" Content="名前：" Grid.Column="1" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Background="White" Height="32" Width="539" Panel.ZIndex="-1" Padding="5,7,5,0"/>
					<Image x:Name="imgBlockCB" Grid.Column="1" HorizontalAlignment="Left" Height="64" Margin="104,76,0,0" VerticalAlignment="Top" Width="64" Source="Assets/icon/noimg.png" MouseLeftButtonDown="imgBlockCB_MouseLeftButtonDown"/>
