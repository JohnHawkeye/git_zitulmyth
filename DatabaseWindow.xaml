					<RadioButton x:Name="rdbObjDisplay" Content="表示のみ" Grid.Column="1" HorizontalAlignment="Left" Margin="110,148,0,0" VerticalAlignment="Top" GroupName="attribute" IsChecked="True" Click="rdbObjDisplay_Click"/>
					<RadioButton x:Name="rdbObjPhysical" Content="物理性質" Grid.Column="1" HorizontalAlignment="Left" Margin="210,148,0,0" VerticalAlignment="Top" GroupName="attribute" Click="rdbObjPhysical_Click"/>
					<RadioButton x:Name="rdbObjCollider" Content="コライダ―" Grid.Column="1" HorizontalAlignment="Left" Margin="320,148,0,0" VerticalAlignment="Top" GroupName="attribute" Click="rdbObjCollider_Click"/>
					<RadioButton x:Name="rdbObjLadder" Content="ラダー" Grid.Column="1" HorizontalAlignment="Left" Margin="110,168,0,0" VerticalAlignment="Top" GroupName="attribute" Click="rdbObjLadder_Click"/>
					<RadioButton x:Name="rdbObjPlat" Content="プラットフォーム" Grid.Column="1" HorizontalAlignment="Left" Margin="210,168,0,0" VerticalAlignment="Top" GroupName="attribute" Click="rdbObjPlat_Click"/>
					<RadioButton x:Name="rdbObjGoal" Content="ゴール" Grid.Column="1" HorizontalAlignment="Left" Margin="320,168,0,0" VerticalAlignment="Top" GroupName="attribute" Click="rdbObjGoal_Click"/>
