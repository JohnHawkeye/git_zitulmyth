					<TextBox x:Name="txbObjAutoX" Grid.Column="1" HorizontalAlignment="Left" Height="20" Margin="151,287,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="40" HorizontalContentAlignment="Right" Panel.ZIndex="2" KeyDown="txbObjAutoX_KeyDown"/>
					<CheckBox x:Name="ckbObjTimer" Content="タイマー：" Grid.Column="1" HorizontalAlignment="Left" Margin="200,236,0,0" VerticalAlignment="Top" Click="ckbObjTimer_Click"/>
					<CheckBox x:Name="ckbObjDamage" Content="ダメージの有無：" Grid.Column="1" HorizontalAlignment="Left" Margin="356,236,0,0" VerticalAlignment="Top" Click="ckbObjDamage_Click"/>
					<TextBox x:Name="txbObjDamage" Grid.Column="1" HorizontalAlignment="Left" Height="20" Margin="462,234,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="64" KeyDown="txbObjDamage_KeyDown"/>
					<CheckBox x:Name="ckbObjAutoMove" Content="自動移動：" Grid.Column="1" HorizontalAlignment="Left" Margin="32,290,0,0" VerticalAlignment="Top" Click="ckbObjAutoMove_Click"/>
					<TextBox x:Name="txbObjAutoY" Grid.Column="1" HorizontalAlignment="Left" Height="20" Margin="225,287,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="40" HorizontalContentAlignment="Right" Panel.ZIndex="2" KeyDown="txbObjAutoY_KeyDown"/>
					<Label x:Name="label" Content="X" Grid.Column="1" HorizontalAlignment="Left" Margin="132,284,0,0" VerticalAlignment="Top" Background="#FFE0E0E0" Width="64"/>
