					<Label x:Name="lblPlayerSpriteName_Copy1" Content="攻撃" HorizontalAlignment="Left" Margin="380,133,0,0" VerticalAlignment="Top" Background="White" Height="26" Width="133" HorizontalContentAlignment="Center" Panel.ZIndex="2"/>
