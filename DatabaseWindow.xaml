					<Image x:Name="imgPlayerDamageL" HorizontalAlignment="Left" Height="64" Margin="46,190,0,0" VerticalAlignment="Top" Width="64" Source="Assets/icon/noimg.png" MouseLeftButtonDown="imgPlayerDamageL_MouseLeftButtonDown" Panel.ZIndex="2" Tag="9"/>
					<Label x:Name="lblPlayerSpriteName_Copy3" Content="ダメージ" HorizontalAlignment="Left" Margin="46,259,0,0" VerticalAlignment="Top" Background="White" Height="26" Width="133" HorizontalContentAlignment="Center" Panel.ZIndex="2"/>
					<Image x:Name="imgPlayerDamageR" HorizontalAlignment="Left" Height="64" Margin="115,190,0,0" VerticalAlignment="Top" Width="64" Source="Assets/icon/noimg.png" MouseLeftButtonDown="imgPlayerDamageR_MouseLeftButtonDown" Panel.ZIndex="2" Tag="10"/>
					<Image x:Name="imgPlayerLadder" HorizontalAlignment="Left" Height="64" Margin="212,190,0,0" VerticalAlignment="Top" Width="64" Source="Assets/icon/noimg.png" MouseLeftButtonDown="imgPlayerLadder_MouseLeftButtonDown" Panel.ZIndex="2" Tag="11"/>
					<Label x:Name="lblPlayerSpriteName_Copy4" Content="昇り降り" HorizontalAlignment="Left" Margin="212,259,0,0" VerticalAlignment="Top" Background="White" Height="26" Width="133" HorizontalContentAlignment="Center" Panel.ZIndex="2"/>
					<Image x:Name="imgPlayerSquatL" HorizontalAlignment="Left" Height="64" Margin="380,190,0,0" VerticalAlignment="Top" Width="64" Source="Assets/icon/noimg.png" MouseLeftButtonDown="imgPlayerSquatL_MouseLeftButtonDown" Panel.ZIndex="2" Tag="13"/>
